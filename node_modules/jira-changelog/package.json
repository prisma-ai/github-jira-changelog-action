  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "commander": "^2.20.0",
    "ejs": "^2.6.1",
    "form-urlencoded": "^2.0.9",
    "haikunator": "^2.1.1",
    "html-entities": "^1.2.1",
    "jira-client": "^6.8.0",
    "lodash": "^4.17.11",
    "node-fetch": "^1.7.3",
    "promise-throttle": "^0.3.1",
    "request": "^2.88.0",
